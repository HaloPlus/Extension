{
  "manifest_version": 3,
  "name": "Halo+",
  "host_permissions": [
    "*://halo.gcu.edu/",
    "*://localhost/",
    "*://halopl.us/"
  ],
  "permissions": [
    "cookies",
    "storage"
  ],
  "minimum_chrome_version": "92",
  "version": "1.1.1",
  "content_scripts": [
    {
      "matches": [
        "https://halo.gcu.edu/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}